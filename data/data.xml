<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <!-- Sequences for sale.order -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Hospital Patient</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_patient_appointment" model="ir.sequence">
            <field name="name"> Patient's Appoinement</field>
            <field name="code">hospital.patient.appointment</field>
            <field name="prefix">HP-AP</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>